CMD:rep(playerid)
{
	if(GetPlayerAdminEx(playerid) >= 1 || GetPlayerYouTubeEx(playerid))
	{
	    if(!g_rep_created) return SendClientMessage(playerid, -1, "На данный момент свободных репортов нет.");

		new fmt_text[360],
			bool: rep_is_found;

			new year, month, day;
			getdate(year, month, day);

		for(new idx; idx < g_rep_created; idx ++)
		{
			if(GetReportData(idx, REP_EDIT) || GetReportData(idx, REP_CREATOR) == INVALID_PLAYER_ID) continue;

			format
			(
				fmt_text, sizeof fmt_text,
				"{FFFF00}| {FFFFFF}Отправитель: {FFFF00}%s[%d]\n"\
	            "{FFFF00}| {FFFFFF}Статус отправителя: {FFFF00}в сети\n"\
	            "{FFFF00}| {FFFFFF}Время и дата: {FFFF00}%s - %02d.%02d.%d\n"\
	            "{FFFF00}| {FFFFFF}Содержимое: {FFFF00}%s\n\n"\
				"{FFFFFF}Вам необходимо сформулировать, а зачем ввести ответ на\n"\
				"жалобу игрока в диалоговое поле ниже.",
				GetPlayerNameEx(GetReportData(idx, REP_CREATOR)),
	            GetReportData(idx, REP_CREATOR),
	            date("%hh:%ii:%ss", RepInfo[idx][RepTime]),
	            day,
	            month,
	            year,
	            GetReportData(idx, REP_TEXT)
			);

			Dialog(playerid, DIALOG_REPORT_EDIT, DIALOG_STYLE_INPUT, "{FF6347}Rostovskiy RP {FFFFFF}| Жалоба", fmt_text, "Ответить", "Удалить");

			SetReportData(idx, REP_EDIT, true);

			SetPVarInt(playerid, "report_id", idx);

			rep_is_found = true;
			
			if(!rep_is_found) return SendClientMessage(playerid, -1, "На данный момент свободных репортов нет.");

			break;
		}
	}
	else ShowNotification(playerid, 2, "У Вас нет доступа к использованию данной команды", 7, "", "");

	return 1;
}

CMD:ask(playerid)
{
    if(GetPlayerHelperEx(playerid) < 1) return ShowNotification(playerid, 2, "У Вас нет доступа к использованию данной команды", 7, "", ""); 
    if(!g_ask_created) return SendClientMessage(playerid, -1, "На данный момент свободных вопросов нет");

	new fmt_text[360],
		bool: ask_is_found;
		
		new year, month, day;
		getdate(year, month, day);

	for(new idx; idx < g_ask_created; idx ++)
	{
		if(GetAskData(idx, ASK_EDIT) || GetAskData(idx, ASK_CREATOR) == INVALID_PLAYER_ID) continue;

		format
		(
			fmt_text, sizeof fmt_text,
			"{FFFFFF}Вы собираетесь ответить на вопрос игрока: {FFFF00}%s[%d]\n\n"\
			"{FFFF00}| {FFFFFF}Текст: {FFFF00}%s\n"\
            "{FFFF00}| {FFFFFF}Дата и время подачи: {FFFF00}%02d-%02d-%d %s\n\n"\
			"{FFFFFF}Вам необходимо сформулировать, а зачем ввести ответ на\n"\
			"вопрос игрока в диалоговое поле ниже.",
			GetPlayerNameEx(GetAskData(idx, ASK_CREATOR)), 
            GetAskData(idx, ASK_CREATOR),
            GetAskData(idx, ASK_TEXT),
            day, 
            month, 
            year,
            date("%hh:%ii:%ss", RepInfo[idx][RepTime])
		);

		Dialog(playerid, DIALOG_ASK_EDIT, DIALOG_STYLE_INPUT, "{FF6347}Rostovskiy RP {FFFFFF}| Вопрос", fmt_text, "Ответить", "Удалить");

		SetAskData(idx, ASK_EDIT, true);

		SetPVarInt(playerid, "ask_id", idx);

		ask_is_found = true;

		break;
	}

	if(!ask_is_found) return SendClientMessage(playerid, -1, "На данный момент свободных вопросов нет");

	return 1;
}

