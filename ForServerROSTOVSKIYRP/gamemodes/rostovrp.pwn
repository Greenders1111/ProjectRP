stock LoadTun(playerid)
{
	new idx = GetPVarInt(playerid, "ownablecar_id"),
	Cache: result,
	query[100];

	mysql_format(mysql, query, sizeof query, "SELECT nitro, launch, fars, diski, gidra FROM ownable_cars WHERE id='%d'", idx);
