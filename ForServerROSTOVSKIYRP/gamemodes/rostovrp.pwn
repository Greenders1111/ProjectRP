alias:ygethere("ygh")

CMD:ygethere(playerid, params[])
{
	if(GetPlayerYouTubeEx(playerid) < 1) return ShowNotification(playerid, 2, "У Вас нет доступа к использованию данной команды", 7, "", "");

	extract params -> new to_player; else return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /ygethere [id игрока]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0x999999FF, "Такого игрока нет");

	if(GetPlayerAdminEx(to_player) > GetPlayerYouTubeEx(playerid))
	{
		SendClientMessage(playerid, 0x66CC33FF, "Вы пытаетесь телепортировать к себе администратора Выше рангом, он должен подтвердить Ваше действие");
		SendPlayerOffer(playerid, to_player, OFFER_TYPE_GETHERE);
	}
	else
	{
		new Float: x, Float: y, Float: z;
		GetPlayerPos(playerid, x, y, z);
		SetPlayerPosEx(to_player, x + 1, y + 1, z, 0.0, GetPlayerInterior(playerid), GetPlayerVirtualWorld(playerid), false);

		SetPlayerInHouse(to_player, GetPlayerInHouse(playerid));
		SetPlayerInBiz(to_player, GetPlayerInBiz(playerid));

		new fmt_msg[105];
		format(fmt_msg, sizeof fmt_msg, "Ютубер %s телепортивал Вас к себе", GetPlayerNameEx(playerid));
		SendClientMessage(to_player, 0x3399FFFF, fmt_msg);

		format(fmt_msg, sizeof fmt_msg, "Вы телепортивали %s к себе", GetPlayerNameEx(to_player));
		SendClientMessage(playerid, 0x3399FFFF, fmt_msg);

		if(GetPlayerAdminEx(playerid) <= 1)
		{
			format(fmt_msg, sizeof fmt_msg, "[Y] Ютубер %s[%d] телепортировал к себе игрока %s[%d]", GetPlayerNameEx(playerid), playerid, GetPlayerNameEx(to_player), to_player);
			SendMessageToAdmins(fmt_msg, 0x999999FF);
		}

	    format(fmt_msg, sizeof fmt_msg, "[Y] Ютубер %s[%d] телепортировал к себе %s[%d]", GetPlayerNameEx(playerid), playerid, GetPlayerNameEx(to_player), to_player);
	    SendMessageToYouTubers(fmt_msg, 0x999999FF);
		
        format(fmt_msg, sizeof fmt_msg, "Телепортировал к себе игрока %s[acc:%d]", GetPlayerNameEx(to_player), GetPlayerAccountID(to_player));
	    SendLog(playerid, LOG_TYPE_ADMIN_ACTION, fmt_msg);
	}

	return 1;
}

alias:ygoto("yg")
