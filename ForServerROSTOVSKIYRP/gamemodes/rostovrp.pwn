IPacket:0x56(playerid, BitStream:bs)
{
	    new header, rpcid, id, specid;

	        BS_ReadValue(bs, PR_UINT8, header, PR_UINT32, rpcid);
	        BS_ReadValue(bs, PR_INT32, id);
			BS_ReadValue(bs, PR_INT32, specid);

			switch(id)
			{
				case 0: HideSpec(playerid);
				case 1: SpecBan(playerid, specid); 
				case 2: SpecSlap(playerid, specid);
				case 3: SpeckWarn(playerid, specid);
				case 4: SpecJail(playerid, specid);
				case 5: SpecKick(playerid, specid);
				case 6: SpecMute(playerid, specid);
			}
}

IPacket:0x59(playerid, BitStream:bs) // адм рекон лр
{
	    new header, rpcid, id, splayerid;

	        BS_ReadValue(bs, PR_UINT8, header, PR_UINT32, rpcid);
	        BS_ReadValue(bs, PR_INT32, id);
			BS_ReadValue(bs, PR_INT32, splayerid);

			switch(id)
			{
				case 0: HideSpec(playerid);
				case 1: StatsSpec(playerid, splayerid);
				case 2: ShowNotification(playerid, 0, "freeze", 2, "", "");
				case 3: ShowNotification(playerid, 0, "unfreze", 2, "", "");
				case 4: { SetPlayerSpawnInit(splayerid); SpawnPlayer(splayerid); }
				case 5: SpecSlap(playerid, splayerid);
				case 6: refresh(playerid, splayerid);
				case 7: SpecMute(playerid, splayerid);
				case 8: SpecJail(playerid, splayerid);
				case 9: SpecKick(playerid, splayerid);
				case 10: SpecBan(playerid, splayerid);
				case 11: SpeckWarn(playerid, splayerid);
				case 12: nextspec(playerid, splayerid);
				case 13: prevspec(playerid, splayerid);
				case 14: callcmd::flip(playerid, "");
			}
}

stock prevspec(playerid, splayerid)
{
	new to_player = splayerid - 1;
	if(splayerid != 0)
	{
	if(GetPlayerAdminEx(playerid) < 1) return 1;

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0x999999FF, "Такого игрока нет");

	if(GetPlayerSecretEx(to_player) >= 1)
		return SendClientMessage(playerid, 0xCECECEFF, "У игрока есть привелегия скрытность");

	if(GetPlayerSpectateData(playerid, S_PLAYER) == -1)
	{
		new Float: x,
			Float: y,
			Float: z,
			Float: a,
			skin = GetPlayerSkin(playerid);

		GetPlayerPos(playerid, x, y, z);
		GetPlayerFacingAngle(playerid, a);

		SetPlayerSpectateData(playerid, S_START_POS_X, x);
		SetPlayerSpectateData(playerid, S_START_POS_Y, y);
		SetPlayerSpectateData(playerid, S_START_POS_Z, z);
		SetPlayerSpectateData(playerid, S_START_ANGLE, a);

		SetPlayerSpectateData(playerid, S_START_INTERIOR, GetPlayerInterior(playerid));
		SetPlayerSpectateData(playerid, S_START_VIRTUAL_WORLD, GetPlayerVirtualWorld(playerid));

		SetSpawnInfo(playerid, 0, skin, x, y, z, a, 0, 0, 0, 0, 0, 0);
	}

	StartSpectate(playerid, to_player);

	new fmt_text[90];

	if(GetPlayerAdminEx(playerid) <= 5)
	{
		format(fmt_text, sizeof fmt_text, "[A] Администратор %s[%d] следит за %s[%d]", GetPlayerNameEx(playerid), playerid, GetPlayerNameEx(to_player), to_player);
		SendMessageToAdmins(fmt_text, 0x999999FF);
	}

	format(fmt_text, sizeof fmt_text, "Следит за %s[acc:%d]", GetPlayerNameEx(to_player), GetPlayerAccountID(to_player));
	SendLog(playerid, LOG_TYPE_ADMIN_ACTION, fmt_text);
	new playername[100];
	format(playername, sizeof playername, "%s", GetPlayerNameEx(to_player));

	return 1;
	}
	else
	{
		return nextspec(playerid, splayerid);
	}
}

