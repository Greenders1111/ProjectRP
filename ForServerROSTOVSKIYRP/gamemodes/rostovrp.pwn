CMD:tune(playerid, params[])
{
    new vehicleid = GetPlayerVehicleID(playerid);
    if(!IsPlayerInAnyVehicle(playerid) || !IsValidVehicle(vehicleid)) return SendClientMessage(playerid, 0xFF0000FF, "Вы должны находиться в автомобиле, чтобы установить прошивку.");
    
    new tuningtype;
    if(!strcmp(params, "sport", true)) tuningtype = 0;
    else if(!strcmp(params, "comfort", true)) tuningtype = 1;
    else if(!strcmp(params, "sport+", true)) tuningtype = 2;
