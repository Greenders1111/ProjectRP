{
if(GetPlayerAdminEx(playerid) == 0)
	{
		if(GetPlayerPremium(playerid) == 0)
		    return  SendClientMessage(playerid, 0xCECECEFF, "У Вас нет VIP аккаунта.");
	}
	
	new fmt_text[200],
		list[sizeof fmt_text * 10 + 50] = "Nick_Name - Должность - Команда - AFK{FFFFFF}";

	for(new i; i < MAX_PLAYERS; i ++)
	{
				new admin_rank[14][70] =
				{
					"Младший Модератор (1)", //1 уровень
					"Модератор (2)", //2 уровень
					"Старший модератор (3)", //3 уровень
					"Администратор (4)", //4 уровень
					"Старший Администратор (5)", //5 уровень
					"Зам. Главного Следящего (6)", //6 уровень
					"Главный Следящий (7)", //7 уровень
					"Куратор Администрации (8)", //8 уровень
					"Заместитель Главного Администратора (9)", //9 уровень
					"Основной Зам. Главного Адм. (10)", //10 уровень
					"Главный Администратор", //11 уровень
					"Спец. Администратор", //12 уровень
					"Команда проекта", //13 уровень
					"Разработчик проекта" //14 уровень
				};
				
				new admin_lvl = GetPlayerAdminEx(i);

		if(!IsPlayerConnected(i)) continue;
		else if(!IsPlayerLogged(i)) continue;
		else if(!(1 <= admin_lvl <= 14)) continue;

		format(fmt_text, sizeof fmt_text, "\n%s [%d] %s (%d lvl)", GetPlayerNameEx(i), i, admin_rank[ GetPlayerAdminEx(i) - 1 ], admin_lvl);

