CMD:mute(playerid, params[])
{
    if(TEST_SERVER == 1) return SendClientMessage(playerid, -1, "{ff2400}| {ffffff}Данная команда не доступна на ТЕСТ СЕРВЕРЕ!");
	if(GetPlayerAdminEx(playerid) < 2) return ShowNotification(playerid, 2, "У Вас нет доступа к использованию данной команды", 7, "", "");
	

	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /mute [id игрока] [время в минутах] [причина]");

	extract params -> new to_player, mute_time, string: reason[30];

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0xCECECEFF, "Такого игрока нет");

	if(!(1 <= mute_time <= 360)) return SendClientMessage(playerid, 0xCECECEFF, "Время должно быть не меньше 1 и не больше 360 минут");
	if(GetPlayerData(to_player, P_MUTE) > 0) return SendClientMessage(playerid, 0xCECECEFF, "У этого игрока уже есть мут");
	if(GetPlayerAdminEx(to_player) > GetPlayerAdminEx(playerid)) return SendClientMessage(playerid, 0xCECECEFF, "Нельзя выдать блокировку чата администратору выше по рангу");
	//else if(to_player == playerid) return SendClientMessage(playerid, 0xCECECEFF, "Нельзя выдать мут самому себе");

	if(AntiSliv(playerid, "/mute")) return true;

	new fmt_msg[128];
	format(fmt_msg, sizeof fmt_msg, "Администратор %s выдал мут игроку %s на %d мин", GetPlayerNameEx(playerid), GetPlayerNameEx(to_player), mute_time);
	
	AddPlayerData(playerid, P_TOTAL_MUTE, +, 1);
    UpdatePlayerDatabaseInt(playerid, "total_mute", GetPlayerData(playerid, P_TOTAL_MUTE));
		
    AddPlayerData(playerid, P_MUTE_FOR_DAY, +, 1);
    UpdatePlayerDatabaseInt(playerid, "mute_for_day", GetPlayerData(playerid, P_MUTE_FOR_DAY));

	if(strlen(reason))
		format(fmt_msg, sizeof fmt_msg, "%s. Причина: %s", fmt_msg, reason);

	SendClientMessageToAll(0xFF5533FF, fmt_msg);

	SetPlayerData(to_player, P_MUTE, mute_time * 60);
	UpdatePlayerDatabaseInt(to_player, "mute", mute_time * 60);

	SendClientMessage(to_player, 0xCECECEFF, "Время до окончания блокировки чата: {CCCC00}/time");

	format(fmt_msg, sizeof fmt_msg, "Выдал %s[acc:%d] мут на %d мин. Причина: %s",
	GetPlayerNameEx(to_player), GetPlayerAccountID(to_player), mute_time, reason);

	SendLog(playerid, LOG_TYPE_ADMIN_ACTION, fmt_msg);

	return 1;
}

CMD:unmute(playerid, params[])
{
    if(TEST_SERVER == 1) return SendClientMessage(playerid, -1, "{ff2400}| {ffffff}Данная команда не доступна на ТЕСТ СЕРВЕРЕ!");
    if(GetPlayerAdminEx(playerid) < 2) return ShowNotification(playerid, 2, "У Вас нет доступа к использованию данной команды", 7, "", "");

	extract params -> new to_player, string:reason[66];
	
	if(!strlen(params)) return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /unmute [id игрока] [причина]");	
	if(!strlen(reason)) return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /unmute [id игрока] [причина]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0xCECECEFF, "Такого игрока нет");

	else if(GetPlayerData(to_player, P_MUTE) <= 0) return SendClientMessage(playerid, 0xCECECEFF, "У этого игрока нет мута");
	else if(to_player == playerid) return SendClientMessage(playerid, 0xCECECEFF, "Нельзя снять мут с самого себя");

	new fmt_msg[200];
	format(fmt_msg, sizeof fmt_msg, "Администратор %s снял блокировку чата с игрока %s", GetPlayerNameEx(playerid), GetPlayerNameEx(to_player));
	if(strlen(reason) > 0)
		format(fmt_msg, sizeof fmt_msg, "%s. Причина: %s", fmt_msg, reason);
	SendClientMessageToAll(0xFF5533FF, fmt_msg);

