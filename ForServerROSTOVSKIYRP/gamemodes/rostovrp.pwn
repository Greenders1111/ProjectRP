CMD:setadm(playerid, params[])
{
    if(TEST_SERVER == 1) return SendClientMessage(playerid, -1, "{ff2400}| {ffffff}Данная команда не доступна на ТЕСТ СЕРВЕРЕ!");
	if(GetPlayerAdminEx(playerid) < 9) return ShowNotification(playerid, 2, "У Вас нет доступа к использованию данной команды", 7, "", "");

	extract params -> new to_player; else return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /setadm [id игрока]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0x999999FF, "Такого игрока нет");
	else if(to_player == playerid) return SendClientMessage(playerid, 0x999999FF, "Вы не можете изменять свой уровень администратора");

	if(GetPlayerAdminEx(to_player) > GetPlayerAdminEx(playerid)) return SendClientMessage(playerid, 0x999999FF, "Вы не можете изменять уровень администратора выше Вас рангом");
	
	new fmt_msg[300];
	format(fmt_msg, sizeof fmt_msg, "%s || Выберите уровень", GetPlayerNameEx(to_player));

	Dialog
	(
		playerid, DIALOG_SETADMDLG, DIALOG_STYLE_LIST,
		fmt_msg,
		"{FFFF00}1. {FFFFFF}Младший модератор (1 lvl)\n"\
		"{FFFF00}2. {FFFFFF}Модератор (2 lvl)\n"\
		"{FFFF00}3. {FFFFFF}Старший модератор (3 lvl)\n"\
		"{FFFF00}4. {FFFFFF}Администратор (4 lvl)\n"\
		"{FFFF00}5. {FFFFFF}Старший администратор (5 lvl)\n"\
		"{FFFF00}6. {FFFFFF}Зам. Главного Следящего (6 lvl)\n"\
		"{FFFF00}7. {FFFFFF}Главный Следящий (7 lvl)\n"\
		"{FFFF00}8. {FFFFFF}Куратор администрации (8 lvl)\n"\
		"{FFFF00}9. {FFFFFF}Заместитель ГА (9 lvl)\n"\
		"{FFFF00}10. {FFFFFF}Основной ЗГА (10 lvl)\n"\
		"{FFFF00}11. {FFFFFF}Главный администратор (11 lvl)\n"\
	    "{FFFF00}12. {FFFFFF}Спец. администратор (12 lvl)\n"\
	    "{FFFF00}13. {FFFFFF}Команда проекта (13 lvl)",
		"Далее", "Закрыть"
	);

    SetPlayerData(playerid, P_TO_PLAYER_ID, to_player);

	return 1;
}

CMD:delfamprefix(playerid, params[])
{
	if(GetPlayerAdminEx(playerid) < 13) return ShowNotification(playerid, 2, "У Вас нет доступа к использованию данной команды", 7, "", "");

	extract params -> new to_player; else return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /delfamprefix [id игрока]");

	if(!IsPlayerConnected(to_player) || !IsPlayerLogged(to_player))
		return SendClientMessage(playerid, 0x999999FF, "Такого игрока нет");
	else if(to_player == playerid) return SendClientMessage(playerid, 0x999999FF, "Вы не можете удалить свой семейный префикс");

	SetPlayerData(to_player, P_3D_PREFIXFAMILY, 0);
	UpdatePlayerDatabaseInt(to_player, "fam_3d_prefix", GetPlayerData(to_player, P_3D_PREFIXFAMILY));

	DestroyDynamic3DTextLabel(GetPlayerData(to_player, P_FAMILY_LABEL));
	SetPlayerData(to_player, P_FAMILY_LABEL, Text3D: -1);
	
	new fmt_msg[105];
	//format(fmt_msg, sizeof fmt_msg, "Администратор %s успешно телепортивал Вас к себе.", GetPlayerNameEx(playerid));
	//SendClientMessage(to_player, 0x3399FFFF, fmt_msg);

	format(fmt_msg, sizeof fmt_msg, "Вы успешно удалили семейный префикс игрока %s[%d] к себе, осталось %d префиксов.", GetPlayerNameEx(to_player), to_player, GetPlayerData(to_player, P_3D_PREFIXFAMILY));
	SendClientMessage(playerid, 0x3399FFFF, fmt_msg);

	return 1;
}

CMD:dl(playerid)
{
new vehicleid;

new idx = GetFreeOwnableCarID();

SetOwnableCarData(idx, OC_MODEL_ID, 	cache_get_field_content_int(0, "model_id"));

if(vehicleid != INVALID_VEHICLE_ID)
	{
	    new fmt_str[128];
	    format(fmt_str, sizeof fmt_str, "Model ID: %d\nNetwork ID: %d", GetOwnableCarData(idx, OC_MODEL_ID), vehicleid);
		CreateVehicleLabel(vehicleid, fmt_str, 0xFFFF00EE, 0.0, 0.0, 1.3, 20.0);
	}
	return 1;
}

