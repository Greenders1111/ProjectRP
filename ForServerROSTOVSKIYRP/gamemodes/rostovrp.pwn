	new c_time = time-(time % 86400);
	
	new fmt_text3[200];

	format(fmt_text3, sizeof fmt_text3, "%d-%02d-%02d %02d:%02d:%02d", year, month, day, hour, minute, second);
	
	new days = 5;

	mysql_format(mysql, fmt_log, sizeof fmt_log, "INSERT INTO car_log (seller_acc_id, seller_name, type, model_name, model_id, buyer_acc_id, buyer_name, money, time, delete_log_time) VALUES (%d, '%s', '%s', '%s', %d, %d, '%s', %d, '%s', %d)",
	seller_acc_id, seller_name, type, model_name, model_id, buyer_acc_id, buyer_name, money, fmt_text3, c_time + (days * 86400));

	mysql_query(mysql, fmt_log, false);

	return 1;
}

stock GetTruckIndex(vehicleid)
{
	if(GetVehicleModel(vehicleid) == 609)
		return 0;

	else if(GetVehicleModel(vehicleid) == 514 || GetVehicleModel(vehicleid) == 584)
		return 1;

	else
		return -1;
}

stock StartSpectate(playerid, for_player)
{
	if(GetPlayerAdminEx(playerid) < 1) return 1;

	SetPlayerSpectateData(playerid, S_PLAYER, for_player);
	
	new fmt_text[300];

	new Float: speed = GetPlayerSpeed(for_player);

	new vehicleid = GetPlayerVehicleID(for_player);

	format(fmt_text, sizeof fmt_text, "%s\t%d~n~HP:\t%f~n~ARMOUR:\t0~n~SPEED:\t%f~n~PING:\t%d~n~MONEY:\t%d~n~CAR\tID:\t%d", GetPlayerNameEx(for_player), for_player, GetPlayerData(for_player, P_HEALTH), speed, GetPlayerPing(for_player), GetPlayerData(for_player, P_MONEY), vehicleid);

	TextDrawSetString(spec_info_TD, fmt_text);

	TextDrawShowForPlayer(playerid, spec_info_TD);
	
	//SpectateInfo(playerid, for_player);

	SetPlayerInterior(playerid, GetPlayerInterior(for_player));
	SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(for_player));

	TogglePlayerSpectating(playerid, true);

	if(IsPlayerInAnyVehicle(for_player))
	{
		PlayerSpectateVehicle(playerid, GetPlayerVehicleID(for_player));
	}
	else PlayerSpectatePlayer(playerid, for_player);

	return 1;
}
