CMD:famsklad(playerid, params[])
{
	if(GetPlayerData(playerid, P_FAMILY) == 0) return SendClientMessage(playerid, 0xCECECEFF, "Вы не состоите в семье");
	
	if(GetPlayerData(playerid, P_LAST_MP) == 1) return Notka(playerid, 2, "Вы не можете использовать склад на мероприятии", 4, "", "");

	new rows, Cache: result, fmt_text[999];
 	new open_close;

	mysql_format(mysql, fmt_text, sizeof fmt_text, "SELECT * FROM family WHERE id='%d'", GetPlayerData(playerid, P_FAMILY) + 1);
	result = mysql_query(mysql, fmt_text, true);

	rows = cache_num_rows();

	if(rows)
	{
		open_close = cache_get_field_content_int(0, "sklad_open_close");

		format
		(
			fmt_text,
			sizeof fmt_text,
			"{FFFF00}| {FFFFFF}Сейф %s\n"\
			"1. Взять\n"\
			"2. Положить\n"\
			"3. Доступы",
			open_close ? ("{01DF01}Открыть") : ("{FF0000}Закрыть")
		);

		Dialog(playerid, DIALOG_FAMILY_SKLAD_DLG, DIALOG_STYLE_LIST, "{FF0000}Сейф семьи", fmt_text, "Выбрать", "Отмена");

		cache_delete(result);

		return 1;
	}
	return 1;
}

public: CloseMpTeleport()
{
    new query[999];

	format(query, sizeof query, "{EDBF23}Телепортация на место проведения мероприятия была автоматически закрыта. Набрано участников: {A5871A}%d из %d{EDBF23}.", mp[MP_KOLVO], mp[MP_MAX_KOLVO]);
	SendClientMessageToAll(-1, query);
	
	mp[MP_CLOSE] = 1;

	return 1;
}

